<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/titleTd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="92dp"
        android:text="Code Next Harlem Coaches"
        android:textSize="30dp"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/instructions"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="52dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/titleTd" />

    <ImageButton
        android:id="@+id/michaelB"
        android:layout_width="133dp"
        android:layout_height="111dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="60dp"
        android:layout_marginRight="58dp"
        android:onClick="toTemplate"
        app:layout_constraintEnd_toStartOf="@+id/uthmanB"
        app:layout_constraintHorizontal_bias="0.83"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/roebuckB"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageButton
        android:id="@+id/uthmanB"
        android:layout_width="133dp"
        android:layout_height="111dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:onClick="toTemplate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/evelynB"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageButton
        android:id="@+id/roebuckB"
        android:layout_width="133dp"
        android:layout_height="111dp"
        android:layout_marginTop="116dp"
        android:layout_marginEnd="60dp"
        android:layout_marginRight="60dp"
        android:onClick="toTemplate"
        app:layout_constraintEnd_toStartOf="@+id/evelynB"
        app:layout_constraintHorizontal_bias="0.83"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/instructions"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageButton
        android:id="@+id/evelynB"
        android:layout_width="133dp"
        android:layout_height="111dp"
        android:layout_marginTop="116dp"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:onClick="toTemplate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/instructions"
        tools:srcCompat="@tools:sample/avatars" />

    <Button
        android:id="@+id/MemeGallery"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="48dp"
        android:text="Meme Gallery"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>